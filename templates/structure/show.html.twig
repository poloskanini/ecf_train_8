{% extends 'base.html.twig' %}

{% block title %}Structure{% endblock %}

{% block body %}
    <h1>Structure</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ structure.id }}</td>
            </tr>
            <tr>
                <th>PostalAdress</th>
                <td>{{ structure.postalAdress }}</td>
            </tr>
            <tr>
                <th>IsPlanning</th>
                <td>{{ structure.isPlanning ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsNewsletter</th>
                <td>{{ structure.isNewsletter ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsBoissons</th>
                <td>{{ structure.isBoissons ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsSms</th>
                <td>{{ structure.isSms ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsConcours</th>
                <td>{{ structure.isConcours ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_structure_index') }}">back to list</a>

    <a href="{{ path('app_structure_edit', {'id': structure.id}) }}">edit</a>

    {{ include('structure/_delete_form.html.twig') }}
{% endblock %}
